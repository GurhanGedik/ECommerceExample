<!DOCTYPE html>

<html>
<head>
    <!-- Basic -->
    <meta charset="UTF-8">

    <title>@ViewBag.Title</title>
    <meta name="keywords" content=" " />
    <meta name="description" content=" ">
    <meta name="author" content="">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/bootstrap-datepicker/css/datepicker3.css" />

    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/Areas/Admin//Theme/vendor/bootstrap-fileupload/bootstrap-fileupload.min.css" />

    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/bootstrap-multiselect/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/morris/morris.css" />

    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/select2/select2.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Theme/vendor/jquery-datatables-bs3/assets/css/datatables.css" />


    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Theme/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="~/Areas/Admin/Theme/vendor/modernizr/modernizr.js"></script>


</head>
<body>

    <section class="body">

        <!-- start: header -->
        <header class="header" id="hdr">
            <div class="logo-container">
                <a href="@Url.Action("Index","Home")" class="logo">
                    <img src="~/Areas/Admin/Theme/images/logo.png" height="35" alt="JSOFT Admin" />
                </a>
                <div class="visible-xs toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">

                <span class="separator"></span>
                @if (Session["FirstName"] != null && Session["LastName"] != null)
                {
                <div id="userbox" class="userbox">
                    <a href="#" data-toggle="dropdown">
                        <figure class="profile-picture">
                            <img src="~/Areas/Admin/Theme/images/!logged-user.jpg" alt="Joseph Doe" class="img-circle" data-lock-picture="~/Areas/Admin/Theme/images/!logged-user.jpg" />
                        </figure>
                        <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@JSOFT.com">
                            <span class="name">@Session["FirstName"].ToString() @Session["LastName"].ToString()</span>
                            <span class="role">administrator</span>
                        </div>

                        <i class="fa custom-caret"></i>
                    </a>

                    <div class="dropdown-menu">
                        <ul class="list-unstyled">
                            <li class="divider"></li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="pages-user-profile.html"><i class="fa fa-user"></i> My Profile</a>
                            </li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="#" data-lock-screen="true"><i class="fa fa-lock"></i> Lock Screen</a>
                            </li>
                            <li>
                                <a role="menuitem" tabindex="-1" href="/Login/SingOut"><i class="fa fa-power-off"></i> Logout</a>
                            </li>
                        </ul>
                    </div>
                </div>

                }
            </div>
            <!-- end: search & user box -->
        </header>
        <!-- end: header -->

        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title" style="color:#abb4be;">
                        Navigation
                    </div>
                    <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">
                        <nav id="menu" class="nav-main" role="navigation">
                            <ul class="nav nav-main">
                                <li class="nav-active">
                                    <a href="@Url.Action("Index","Home")">
                                        <i class="fa fa-home" aria-hidden="true"></i>
                                        <span>Dashboard</span>
                                    </a>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-users" aria-hidden="true"></i>
                                        <span>Product</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a href="@Url.Action("List","Product")">
                                                Product List
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-users" aria-hidden="true"></i>
                                        <span>Category</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a href="@Url.Action("List","Category")">
                                                Category List
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-users" aria-hidden="true"></i>
                                        <span>Customer</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a href="@Url.Action("List","Customer")">
                                                Customer List
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="nav-parent">
                                    <a>
                                        <i class="fa fa-users" aria-hidden="true"></i>
                                        <span>Manufacturer</span>
                                    </a>
                                    <ul class="nav nav-children">
                                        <li>
                                            <a href="@Url.Action("List","Manufacturer")">
                                                Manufacturer List
                                            </a>
                                        </li>
                                    </ul>
                                </li>



                            </ul>
                        </nav>

                    </div>

                </div>

            </aside>
            <!-- end: sidebar -->

            <section role="main" class="content-body">
                @RenderBody()
            </section>
        </div>


    </section>




    <!-- Vendor -->
    <script src="~/Areas/Admin/Theme/vendor/jquery/jquery.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Specific Page Vendor -->
    <script src="~/Areas/Admin/Theme/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-appear/jquery.appear.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/bootstrap-multiselect/bootstrap-multiselect.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-easypiechart/jquery.easypiechart.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/flot/jquery.flot.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/flot-tooltip/jquery.flot.tooltip.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/flot/jquery.flot.pie.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/flot/jquery.flot.categories.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/flot/jquery.flot.resize.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-sparkline/jquery.sparkline.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/raphael/raphael.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/morris/morris.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/gauge/gauge.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/snap-svg/snap.svg.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/liquid-meter/liquid.meter.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/jquery.vmap.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/data/jquery.vmap.sampledata.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/jquery.vmap.world.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.africa.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.asia.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.australia.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.europe.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.north-america.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>
    <!-- Specific Page Vendor -->
    <script src="~/Areas/Admin/Theme/vendor/select2/select2.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-datatables/media/js/jquery.dataTables.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-datatables/extras/TableTools/js/dataTables.tableTools.min.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/jquery-datatables-bs3/assets/js/datatables.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="~/Areas/Admin/Theme/javascripts/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/Areas/Admin/Theme/javascripts/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/Areas/Admin/Theme/javascripts/theme.init.js"></script>

    <!-- Specific Page Vendor -->
    <script src="~/Areas/Admin/Theme/vendor/jquery-autosize/jquery.autosize.js"></script>
    <script src="~/Areas/Admin/Theme/vendor/bootstrap-fileupload/bootstrap-fileupload.min.js"></script>

    <!-- Examples -->
    <script src="~/Areas/Admin/Theme/javascripts/dashboard/examples.dashboard.js"></script>

    <!-- Examples -->
    <script src="~/Areas/Admin/Theme/javascripts/tables/examples.datatables.default.js"></script>
    <script src="~/Areas/Admin/Theme/javascripts/tables/examples.datatables.row.with.details.js"></script>
    <script src="~/Areas/Admin/Theme/javascripts/tables/examples.datatables.tabletools.js"></script>


</body>
</html>
